        "def getCrossValidator(estimator, params, folds):\n",
        "  return CrossValidator(\n",
        "      estimator=estimator,\n",
        "      estimatorParamMaps=params,\n",
        "      evaluator=evaluator,\n",
        "      numFolds=folds,\n",
        "      seed=101\n",
        "  )"
      ]
